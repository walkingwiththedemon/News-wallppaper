    "dependencies": {
        "prop-types": "^15.6.1",
        "react": "^16.2.0",
        "react-cli": "^0.3.1",
        "react-dom": "^16.2.0",
        "react-icons": "^2.2.7",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.2.2",
        "redux": "^3.7.2",
        "uuid": "^3.2.1"
    },
    "jest": {
        "setupFiles": [
            "./__tests__/shim.js",
            "./__tests__/global.js"
        ],
        "modulePathIgnorePatterns": [
            "shim.js",
            "global.js"
        ],
        "verbose": true,
        "collectCoverage": true,
        "notify": true,
        "collectCoverageFrom": [
            "src/**"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 0,
                "functions": 0,
                "lines": 0,
                "statements": 0
            }
        },
        "moduleNameMapper": {
            "\\.(sass)$": "<rootDir>/node_modules/jest-css-modules"
        }
    }
